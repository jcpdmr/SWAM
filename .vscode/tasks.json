{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Maven Install (Skip Tests)",
            "type": "shell",
            "command": "./mvnw",
            "args": [
                "install",
                "-DskipTests"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared"
            },
            "problemMatcher": []
        }
    ]
}